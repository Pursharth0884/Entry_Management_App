

<!DOCTYPE html>
<html lang='en' class=''>
<head>
<title>Records | Office Way!</title>
<meta charset='UTF-8'>
<meta name="robots" content="noindex">


<script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">
<link rel="stylesheet" href="css/view-record/style.css">
</head>
<body>
<div id="demo">
<h1>Office way!</h1>
<h2>List of the Visitors </h2>


<div class="table-responsive-vertical shadow-z-1">

<table id="table" class="table table-hover table-mc-light-blue">
<thead>
<tr>
<th>ID</th>
<th>Name</th>
<th>Status</th>
<th>Action</th>
</tr>
</thead>
<tbody>


<% console.log("new page") %>

<% newVisitors.forEach(function(visitor){ %>
    <tr>
    <form action="/view" method="post" id="myform">
      <td data-title="ID" name="id" value="<%= visitor._id %>"> <%= visitor._id %> </td>
      <td data-title="Name"><%= visitor.visitorName %></td>
      <% console.log("for " + visitor._id +" "+visitor.isCheckout) %>
     <% if(visitor.isCheckout){ %>
          <td data-title="Status">Checked Out!</td>
          <td data-title="Link">
          <span>-</span>
          </td>
      
        <% } else { %>
          <td data-title="Status">Available</td>
          <td data-title="Link">
          <button type="submit" name="id" value="<%= visitor._id %>">Check out?</button> 
          <!-- <a onclick=" $(#myform).submit(); return false;">Check out?</a> -->
          </td>
       <% } %>
      
    </form>
     </tr>
  <% }) %>

  
  
</tbody>
</table>

</div>

<div class="button">
<a href="/home" class="btn btn-bubble">Return to Home</a>
</div>

<script src="https://static.codepen.io/assets/common/stopExecutionOnTimeout-de7e2ef6bfefd24b79a3f68b414b87b8db5b08439cac3f1012092b2290c719cd.js"></script><script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script src="js/view-record/main.js" type="text/javascript"></script>


</body>
</html>

  